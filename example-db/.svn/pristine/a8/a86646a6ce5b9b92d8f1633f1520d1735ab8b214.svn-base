package bg.infosys.crc.admin.dto.reports;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;

import lombok.Getter;
import lombok.NoArgsConstructor;

@Getter
@NoArgsConstructor
@JsonInclude(Include.NON_NULL)
public class Report1DTO {
	private String networkType;
	private String region;
	
	private Integer speedDownload;
	private SpeedItemsDTO items;
	
	public Report1DTO(Integer speedDownload, String region, String networkType, Object items) {
		this.speedDownload = speedDownload != null ? speedDownload / 1000 : null; // kbps to mbps
		this.region = region;
		this.networkType = networkType;
		this.items = (SpeedItemsDTO) items;
	}
}
