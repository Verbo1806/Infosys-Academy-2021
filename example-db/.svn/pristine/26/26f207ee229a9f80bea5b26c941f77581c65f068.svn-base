package bg.infosys.crc.admin.entity;

import java.time.LocalDateTime;
import java.util.UUID;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;

@Entity
@Table(name = "test")
@Getter
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class Test {
	@Id @GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long uid;
	public static final String _uid = "uid";

	@Column(name = "uuid")
	private UUID uuid;
	public static final String _uuid = "uuid";
	
	/*
	 * @Column(name = "open_test_uuid") private UUID openTestUuid; public static
	 * final String _openTestUuid = "openTestUuid";
	 */

	@Column(name = "time")
	private LocalDateTime time;
	public static final String _time = "time";
	
	@Column(name = "ping_median")
	private Long pingMedian;
	public static final String _pingMedian = "pingMedian";
	
	@Column(name = "speed_upload")
	private Integer speedUpload;
	public static final String _speedUpload = "speedUpload";
	
	@Column(name = "speed_download")
	private Integer speedDownload;
	public static final String _speedDownload = "speedDownload";
	
	@Column(name = "signal_strength")
	//@Column(name = "lte_rsrp")
	private Integer signalStrength;
	public static final String _signalStrength = "signalStrength";
	
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "provider_id")
	private Provider provider;
	public static final String _provider = "provider";
	
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "region_code")
	private Region region;
	public static final String _region = "region";
	
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "network_type")
	private NetworkType networkType;
	public static final String _networkType = "networkType";
	
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "open_test_uuid")
	private Speed2 speed;
	public static final String _speed = "speed";
}
