package bg.infosys.crc.admin.dto;

import com.fasterxml.jackson.annotation.JsonInclude;
import com.fasterxml.jackson.annotation.JsonInclude.Include;

import io.swagger.annotations.ApiModelProperty;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;

@Getter
@Builder
@NoArgsConstructor
@AllArgsConstructor
@JsonInclude(Include.NON_NULL)
public class MeasurementDTO {
	@ApiModelProperty(notes = "Ping in milliseconds", example = "110", required = true, position = 0)
	private Integer ping;
	
	@ApiModelProperty(notes = "Download speed in Megabits per second (Mbps)", example = "18", required = true, position = 1)
	private Integer downloadSpeed;
	
	@ApiModelProperty(notes = "Upload speed in Megabits per second (Mbps)", example = "1", required = true, position = 2)
	private Integer uploadSpeed;
	
	@ApiModelProperty(notes = "!", example = "!", required = true, position = 3)
	private Integer provider;
}
