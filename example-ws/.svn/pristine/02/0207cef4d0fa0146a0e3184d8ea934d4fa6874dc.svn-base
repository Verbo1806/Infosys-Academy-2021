package bg.infosys.crc.admin.service.mapper;

import org.springframework.stereotype.Service;

import bg.infosys.crc.admin.dto.NetworkTypeDTO;
import bg.infosys.crc.admin.entity.NetworkType;

@Service
public class NetworkTypeMapper implements IModelMapper<NetworkTypeDTO, NetworkType> {

	@Override
	public NetworkTypeDTO toDto(NetworkType entity) {
		return NetworkTypeDTO.builder()
						  .uid(entity.getUid())
						  .name(entity.getName())
						  .build();
	}

	@Override
	public NetworkType toEntity(NetworkTypeDTO dto) {
		return NetworkType.builder()
						.uid(dto.getUid())
						.name(dto.getName())
						.build();
	}

}
